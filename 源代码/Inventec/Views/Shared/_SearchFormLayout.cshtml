    @RenderSection("scripts", required: false)
    <script>
        $(function () {           
            var dialog = parent.dialog.get(window);
            var data = dialog.data; // 获取对话框传递过来的数据
            $('.searchlisttable tbody tr').dblclick(function () {
                var val = $(this).find("input").eq(0).val();
                dialog.close(val);
                dialog.remove();
            });
          
          
        });
    </script>
